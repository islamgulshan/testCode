{
  "name": "genesis-lab-admin-backend",
  "version": "1.0.0",
  "description": "Genesis Lab Admin Backend",
  "author": "GenesisLab",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts .",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "clear_jest": "jest --clearCache",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "webpack": "webpack --config webpack.config.js",
    "ts-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:create": "typeorm migration:create -d src/migrations",
    "migration:run": "yarn ts-typeorm migration:run",
    "migration:revert": "yarn ts-typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.7.1",
    "@nestjs/bull": "^0.5.3",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "authenticator": "^1.1.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "country-state-city": "^3.0.1",
    "cross-env": "^7.0.3",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "bull": "^4.9.0",
    "dotenv": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "libphonenumber-js": "^1.9.51",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^3.1.3",
    "nodemailer": "^6.7.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.2.30",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^3.11.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "repository": "git+ssh://git@bitbucket.org/rnssol/genesislab-admin-backend.git",
  "bugs": {
    "url": "https://bitbucket.org/rnssol/genesislab-admin-backend/issues"
  },
  "homepage": "https://bitbucket.org/rnssol/genesislab-admin-backend#readme"
}
